{% extends 'base.html.twig' %}

{% block title %}Create an Object{% endblock %}
{% block metadesc %}Form to create an Object{% endblock %}

{% block pageWrapper %}equip{% endblock %}

{% block body %}

<h1>Create an Object</h1>

{# read and display all flash messages #}
{% for label, messages in app.flashes %}
    {% for message in messages %}
        <div class="flash-{{ label }}">
            {{ message }}
        </div>
    {% endfor %}

{% endfor %}

{{ form_start(createEquipForm) }}
    {{ form_errors(createEquipForm) }}
    
    {# {{ form_row(createEquipForm.title) }} #}

    {{ form_row(createEquipForm.name) }}
    {{ form_row(createEquipForm.text) }}
    {{ form_row(createEquipForm.sub_category) }}
    {{ form_row(createEquipForm.path) }}

    
    <button type="submit" class="form-btn-submit">Submit</button>
{{ form_end(createEquipForm) }}

{% endblock %}
